{% extends "base.html.twig" %}


{% block title 'Modifier un événement' %}

{% block body %}
	<h1 class="display-4 text-center shadow p-3 mb-5 bg-white rounded">
		Modification Evénement
	</h1>

	<div class="container shadow p-3 mb-5 bg-white rounded col-sm-6 col-sm-push-3">
	{{ form_start(formEvent) }}
		{{ form_widget(formEvent) }}
		<button type="submit" class="btn btn-primary">
			<span class="material-icons">
				edit
			</span>Modifier</button>
	</div>
	{{ form_end(formEvent) }}

{% endblock %}
